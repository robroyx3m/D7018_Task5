    "version": "0.2.0",
    "configurations": [

        {
            "type": "gdb",
            "request": "attach",
            "name": "Debug",
            "gdbpath": "/usr/bin/arm-none-eabi-gdb",
            "executable": "./target/thumbv7em-none-eabihf/debug/task5",
            "target": ":3333",
            "remote": true,
            "autorun": [
                "monitor reset init",
                "monitor arm semihosting enable",
                "monitor tpiu config internal /tmp/itm.log uart off 64000000",
                "monitor itm port 0 on",
                "load",
                "monitor reset init"
            ],
            "cwd": "${workspaceRoot}"
        },
        {
            "type": "gdb",
            "request": "attach",
            "name": "Release",
